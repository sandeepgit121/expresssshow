                CitiesHTML += "<li id='c_" + CityId + "_"+CityName+"' style='background-color: #E7E2E2; cursor: pointer; padding-left: 5px;color: black;' onclick=setCity(this.id) onmousedown=onCityMouseDown(this.id)onmouseup=onCityMouseUp(this.id)>" + CityName + "</li>";
            }
            document.getElementById("CitiesUI").innerHTML = CitiesHTML;
        }

        function dataBetween(Text, FirstString, LastString) {
            var i, j;

            FirstString = FirstString.toUpperCase();
            LastString = LastString.toUpperCase();
            Text = Text.toUpperCase();

            if ((FirstString == "") || (LastString == "")) {
                return '';
            };

            i = Text.indexOf(FirstString);
            if (i >= 0) {
                Text = Text.substring(i + FirstString.length);
            }
            else {
                return '';
            }

            if (LastString) {
                i = Text.indexOf(LastString);
                if (i >= 0) {
                    Text = Text.substring(0, i);
                }
                else {
                    return '';
                }
            }
            return Text;
        }
